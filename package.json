{
  "name": "patron.js",
  "version": "3.2.0",
  "description": "The cleanest command framework for discord.js and eris.",
  "type": "module",
  "engines": {
    "node": ">=12.13.1"
  },
  "main": "./lib/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "preinstall": "node lib/preinstall.js",
    "postinstall": "node lib/postinstall.js",
    "preuninstall": "node lib/uninstall.js",
    "postuninstall": "node lib/uninstall.js",
    "lint": "eslint lib --ext .js && eslint site --ext .js",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LJNeon/patron.js.git"
  },
  "keywords": [
    "discord",
    "api",
    "nodejs",
    "discordjs",
    "eris",
    "command",
    "framework"
  ],
  "author": "ElJay#7711",
  "license": "AGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/LJNeon/patron.js/issues"
  },
  "homepage": "https://github.com/LJNeon/patron.js#readme",
  "devDependencies": {
    "eslint": "^6.6.0",
    "jsdoc": "^3.6.3",
    "puppeteer": "^2.0.0"
  }
}
